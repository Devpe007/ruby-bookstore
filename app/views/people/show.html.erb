<p  id="notice" style="color: green"><%= notice %></p>

<p>
  <strong> <%= Person.human_attribute_name(:name) %> </strong>
  <%= @person.name %>
</p>

<p>
  <strong> <%= Person.human_attribute_name(:email) %> </strong>
  <%= mail_to @person.email, nil, encode: 'hex' %>
</p>

<p>
  <strong> <%= Person.human_attribute_name(:password) %> </strong>
  <%= @person.password %>
</p>

<p>
  <strong> <%= Person.human_attribute_name(:born_at) %> </strong>
  <%= @person.born_at %>
</p>

<p>
  <strong> <%= Person.human_attribute_name(:admin) %> </strong>
  <%= @person.admin %>
</p>

<p>
  <%= @person.image %>
</p>

