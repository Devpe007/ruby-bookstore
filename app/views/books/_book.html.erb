<div id="<%= dom_id book %>">
  <p>
    <strong>Title:</strong>
    <%= book.title %>
  </p>

  <p>
    <strong>Published at:</strong>
    <%= book.published_at %>
  </p>

  <p>
    <strong>Text:</strong>
    <%= book.text %>
  </p>

  <p>
    <strong>Value:</strong>
    <%= book.value %>
  </p>

  <p>
    <strong>Person:</strong>
    <%= book.person_id %>
  </p>

</div>
